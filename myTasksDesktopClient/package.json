  "version": "0.0.1",
  "description": "",
  "main": "Gruntfile.js",
  "directories": {
    "test": "test"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.min.js",
    "angular-route": "./bower_components/angular-route/angular-route.min.js",
    "angular-ui-router": "./bower_components/angular-ui-router/release/angular-ui-router.min.js",
    "angular-resource": "./bower_components/angular-resource/angular-resource.min.js",
    "angular-bootstrap": "./bower_components/angular-bootstrap/ui-bootstrap.min.js",
