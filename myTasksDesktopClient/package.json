    "ng-tags-input": "./bower_components/ng-tags-input/ng-tags-input.min.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-route": {
      "exports": "angular.module('ngRoute')",
      "depends": "angular"
    },
    "angular-ui-router": {
      "exports": "angular.module('ui.router')",
      "depends": "angular"
    },
    "angular-resource": {
      "exports": "angular.module('ngResource')",
      "depends": "angular"
    },
    "angular-bootstrap": {
      "exports": "angular.module('ui.bootstrap')",
      "depends": "angular"
    },
    "ng-table": {
      "exports": "angular.module('ngTable')",
      "depends": "angular"
    },
    "ng-tags-input": {
      "exports": "angular.module('ngTagsInput')",
      "depends": "angular"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {},
  "author": "Florent FREMONT",
  "devDependencies": {
    "grunt": "~0.4.5",
